    "editorOverviewRuler.infoForeground": "#B3E5E5"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#9999BB",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#CDEBCD"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#F0D9B8"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#E5B3E5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function / Method",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#B3E5E5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class / Type",
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": {
        "foreground": "#F5F5B5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "identifier"],
      "settings": {
        "foreground": "#D6FFFF"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FFD6D6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property / Object Key",
      "scope": ["variable.other.property", "meta.object-literal.key"],
      "settings": {
        "foreground": "#D6FFD6"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#FFEAD6"
      }
    },
    {
      "name": "Operator / Symbol",
      "scope": ["keyword.operator", "meta.operator"],
      "settings": {
        "foreground": "#E5B3E5"
      }
    },
    {
      "name": "HTML / XML Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#E5B3E5"
      }
    },
    {
      "name": "HTML / JSX Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#B3E5E5"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#E5B3E5"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#B3E5E5"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#E5B3E5"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#E5B3E5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#B3E5E5",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#B3E5B3",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid / Deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#FF9999",
        "background": "#FF999930"
      }
    }
  ]
}
