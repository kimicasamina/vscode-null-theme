    "editorOverviewRuler.border": "#0D0D0D",
    "editorOverviewRuler.modifiedForeground": "#FFFF00",
    "editorOverviewRuler.addedForeground": "#00FF80",
    "editorOverviewRuler.deletedForeground": "#FF0040",
    "editorOverviewRuler.errorForeground": "#FF0040",
    "editorOverviewRuler.warningForeground": "#FFFF00",
    "editorOverviewRuler.infoForeground": "#00F5FF"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#A6A6A6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#32FF7E"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#FF1493",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#1E90FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": {
        "foreground": "#ADFF2F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#87CEFA"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter", "parameter"],
      "settings": {
        "foreground": "#FF6347",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "support.type.property-name"],
      "settings": {
        "foreground": "#98FB98"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.other"],
      "settings": {
        "foreground": "#FFA500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#FF4500"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type", "storage.type"],
      "settings": {
        "foreground": "#9370DB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import/Export",
      "scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
      "settings": {
        "foreground": "#FF69B4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#FF4500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#4682B4"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css", "entity.other.attribute-name.class.css"],
      "settings": {
        "foreground": "#FF6347",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#32CD32"
      }
    },
    {
      "name": "CSS Value",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#87CEEB"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FFA07A"
      }
    },
    {
      "name": "JSX Component",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#20B2AA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regular Expression",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#DC143C"
      }
    },
    {
      "name": "Template Literal",
      "scope": ["string.template", "punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#40E0D0"
      }
    },
    {
      "name": "Decorator",
      "scope": ["entity.name.function.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#DAA520",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#5F9EA0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#7FFFD4"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#8A2BE2"
      }
    },
    {
      "name": "Error",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#FF8C00",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
