    "editorOverviewRuler.warningForeground": "#FFFFAA",
    "editorOverviewRuler.infoForeground": "#AAAAFF"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#9999CC",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#90EE90"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#DAA520",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#EE82EE",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#4682B4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#B0C4DE"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FF7F50",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#00FA9A"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant"],
      "settings": {
        "foreground": "#BA55D3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF4500"
      }
    },
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#DC143C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#4682B4"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#DA70D6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#20B2AA"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#B22222",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#9932CC",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#1E90FF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#32CD32",
        "fontStyle": "bold"
      }
    }
  ]
}
