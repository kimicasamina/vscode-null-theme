  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#7777AA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#66FFAA"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFAA66"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#FF99FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function / Method",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#9999FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class / Type",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#FFFFAA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "identifier"],
      "settings": {
        "foreground": "#CCCCFF"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FFBBDD",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property / Key",
      "scope": ["variable.other.property", "meta.object-literal.key"],
      "settings": {
        "foreground": "#BBFFBB"
      }
    },
    {
      "name": "Constant / Enum",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#FFCC99"
      }
    },
    {
      "name": "Operator / Symbol",
      "scope": ["keyword.operator", "meta.operator"],
      "settings": {
        "foreground": "#FF99FF"
      }
    },
    {
      "name": "HTML / XML Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#FF99FF"
      }
    },
    {
      "name": "Attribute (HTML, JSX)",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#AAAAFF"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#FF99FF"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#9999FF"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FF99FF"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FF99FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#9999FF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#66FF99"
      }
    },
    {
      "name": "Invalid / Deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#FF3366",
        "background": "#FF336620"
      }
    }
  ]
}
