  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#888888",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#7FFFD4", // brighter aquamarine for strings
        "fontStyle": "italic"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFA500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword - Control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Keyword - Declaration",
      "scope": ["keyword.declaration"],
      "settings": {
        "foreground": "#FFAA00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Keyword - Export Default",
      "scope": ["keyword.export", "keyword.default"],
      "settings": {
        "foreground": "#FF4500",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#00BFFF",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#ADFF2F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#ADD8E6"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#F0E68C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#98FB98",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant"],
      "settings": {
        "foreground": "#DDA0DD",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF6347",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#FF4500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#20B2AA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#FF1493",
        "fontStyle": "bold"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#5F9EA0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FA8072",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#9370DB",
        "fontStyle": "bold underline"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4169E1",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#7CFC00",
        "fontStyle": "bold"
      }
    }
  ]
}
