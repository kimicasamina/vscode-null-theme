    "editorOverviewRuler.errorForeground": "#FF4400",
    "editorOverviewRuler.warningForeground": "#FFDD00",
    "editorOverviewRuler.infoForeground": "#00DDFF"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#AAAAAA",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#00FF66"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFA500"
      }
    },
    {
      "name": "Keyword / Control Flow",
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function and Method Names",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#00EEFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class / Type",
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": {
        "foreground": "#FFFF55"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "identifier"],
      "settings": {
        "foreground": "#66FFFF"
      }
    },
    {
      "name": "Function Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FF6A6A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Object Properties",
      "scope": ["variable.other.property", "meta.object-literal.key"],
      "settings": {
        "foreground": "#99FF66"
      }
    },
    {
      "name": "Constants / Enums",
      "scope": ["constant", "variable.other.constant"],
      "settings": {
        "foreground": "#FF9900"
      }
    },
    {
      "name": "Operators / Symbols",
      "scope": ["keyword.operator", "meta.operator"],
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "name": "Tags (HTML/XML)",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#FFDD00"
      }
    },
    {
      "name": "Attributes (HTML/CSS/JSX)",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00DDFF"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#FFCC00"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#00DDEE"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FFDD00"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FFDD00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#00DDFF",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#00FF44"
      }
    },
    {
      "name": "Invalid / Deprecated",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#FF3333",
        "background": "#FF333320"
      }
    }
  ]
}
