  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#777777",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#00FF66"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FF6600"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#00FFFF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#FFFF00",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#66FFFF"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FF6666"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#66FF66"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant"],
      "settings": {
        "foreground": "#FF9900"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "HTML Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "HTML Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "CSS Selector",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "CSS Property",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "name": "Markdown Heading",
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FF00FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#00FF66"
      }
    }
  ]
}
